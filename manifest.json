{
  "manifest_version": 3,
  "name": "SimpleExtraction",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "wrappers.js",
        "extraction.js",
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
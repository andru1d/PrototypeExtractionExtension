{
  "manifest_version": 3,
  "name": "SimpleExtraction",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "extraction.js",
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  }
}